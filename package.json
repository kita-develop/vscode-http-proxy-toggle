{
  "name": "http-proxy-toggle",
  "displayName": "http-proxy-toggle",
  "description": "VSCodeの設定にあるHTTPプロキシ設定をショートカットキーで切り替える",
  "publisher": "kita",
  "icon": "proxyIcon.png",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "activation": {
    "property": "http",
    "command": "http-proxy-toggle.toggleProxy"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "title": "Toggle HTTP Proxy",
        "command": "http-proxy-toggle.toggleProxy",
        "when": "editorFocus",
        "keybindings": [
            { "key": "ctrl+shift+p" }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.89.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.9"
  }
}
